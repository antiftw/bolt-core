  },
  "main": "webpack.config.js",
  "directories": {
    "tests": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bolt/core.git"
  },
  "overrides": {
    "@achrinza/node-ipc": "^9.2.6"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
