    "conflict": {
        "symfony/dependency-injection": "6.7",
        "symfony/security-http": "v6.0 v6.1",
        "cocur/slugify": "4.5.0"
    },
    "require-dev": {
        "ext-curl": "*",
        "ext-pdo_sqlite": "*",
        "acmecorp/reference-extension": "^1.2",
        "bobdenotter/configuration-notices": "^1.2",
        "bobdenotter/weatherwidget": "^1.1",
        "bolt/newswidget": "^1.3",
        "coduo/php-matcher": "^6.0.15",
        "dama/doctrine-test-bundle": "^8.2",
        "nyholm/psr7": "^1.4",
        "ondram/ci-detector": "^4.1",
        "php-http/curl-client": "^2.2",
        "php-http/discovery": "^1.14",
        "php-http/httplug-bundle": "^1.19",
        "php-http/message": "^1.12",
        "php-translation/loco-adapter": "dev-master",
        "phpstan/phpstan": "^1.2.0",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-symfony": "^1.0.1",
        "phpunit/phpunit": "^11.0",
        "se/selenium-server-standalone": "^3.141",
        "symfony/browser-kit": "^7",
        "symfony/css-selector": "^7",
        "symplify/easy-coding-standard": "^10.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "drupol/composer-packages": true,
            "symfony/flex": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "public-dir": "public",
        "symfony": {
            "allow-contrib": true,
            "require": "^5.2"
        }
    },
    "autoload": {
        "psr-4": {
            "Bolt\\": "src/",
            "Bolt\\ComposerScripts\\": "bin/composer-script/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Tests\\": "tests/php/",
            "spec\\Bolt\\": "tests/spec/Bolt/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "Bolt\\ComposerScripts\\ProjectEventHandler::corePostInstall"
        ],
        "post-update-cmd": [
            "Bolt\\ComposerScripts\\ProjectEventHandler::corePostUpdate"
        ],
        "pre-package-uninstall": [
            "Bolt\\ComposerScripts\\ProjectEventHandler::prePackageUninstall"
        ]
    },
    "repositories": {
        "jfsenechal_doctrine_behaviors": {
            "type": "vcs",
            "url": "git@github.com:jfsenechal/DoctrineBehaviors.git"
        },
        "antiftw-yaml-migrations": {
            "type": "vcs",
            "url": "https://github.com/antiftw/bdo-yaml-migrations"
        },
        "antiftw-php-translation-symfony-bundle": {
            "type": "vcs",
            "url": "https://github.com/antiftw/php-translation-symfony-bundle"
        },
        "antiftw-php-translation-extractor": {
            "type": "vcs",
            "url": "https://github.com/antiftw/php-translation-extractor"
        },
        "antiftw-php-translation-loco-adapter": {
            "type": "vcs",
            "url": "https://github.com/antiftw/php-translation-loco-adapter"
        }
    }
}
